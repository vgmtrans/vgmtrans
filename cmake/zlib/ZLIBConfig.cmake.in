# Autogenerated configuration file to expose the vendored zlib-ng build as a
# ZLIB package for subprojects such as minizip-ng.

set(ZLIB_FOUND TRUE)
set(ZLIB_VERSION "@ZLIB_VERSION@")
set(ZLIB_VERSION_STRING "@ZLIB_VERSION@")
set(ZLIB_INCLUDE_DIR "@ZLIB_INCLUDE_DIR@")
set(ZLIB_INCLUDE_DIRS "@ZLIB_INCLUDE_DIRS@")

if(NOT TARGET ZLIB::ZLIB)
  add_library(ZLIB::ZLIB INTERFACE IMPORTED)
endif()

set_target_properties(ZLIB::ZLIB PROPERTIES
  INTERFACE_INCLUDE_DIRECTORIES "@ZLIB_INCLUDE_DIRS@"
  INTERFACE_LINK_LIBRARIES "zlib"
)

set(ZLIB_LIBRARIES ZLIB::ZLIB)
set(ZLIB_LIBRARY ZLIB::ZLIB)
